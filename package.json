{
  "name": "formula-one-wrapper",
  "version": "0.1.0",
  "description": "A wrapper of the Ergast API for Node.js powered by TypeScript",
  "main": "./lib/index.js",
  "type": "module",
  "keywords": [
    "f1",
    "formula1",
    "formula-one",
    "ergast"
  ],
  "scripts": {
    "build:client": "rimraf ./lib && tsc",
    "build:website": "turbo run build",
    "start": "npm run build && node lib/index.js",
    "dev:cliet": "npx nodemon",
    "dev:website": "turbo run dev",
    "lint": "turbo run lint",
    "test": "jest",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ultimatehecker/Formula-One-Wrapper.git"
  },
  "author": "ultimate_hecker#1165",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/ultimatehecker/Formula-One-Wrapper/issues"
  },
  "homepage": "https://github.com/ultimatehecker/Formula-One-Wrapper#readme",
  "devDependencies": {
    "@types/async": "^3.2.18",
    "@types/axios": "^0.14.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.10",
    "@types/underscore": "^1.11.4",
    "eslint-config-custom": "^0.0.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "rimraf": "^4.4.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "turbo": "^1.8.6",
    "typescript": "^5.0.3"
  },
  "dependencies": {
    "async": "^3.2.4",
    "axios": "^1.3.4",
    "underscore": "^1.13.6"
  }
}
